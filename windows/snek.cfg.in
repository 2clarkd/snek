[Application]
name=Snek Development Environment
version=@SNEK_VERSION@
script=snekde.py
icon=snek.ico
console=true

[Shortcut Snek]
target=snek.exe
console=true
icon=snek.ico

[Python]
version=3.7.0

[Include]
pypi_wheels=pyserial==3.4
	windows-curses==1.0

files =../COPYING
	../doc/snek.pdf
	../snek-duino/snek-duino-@SNEK_VERSION@.hex
	../metro-snek/metro-snek-@SNEK_VERSION@.uf2
	../examples

[Build]
installer_name=snek-windows-@SNEK_VERSION_DASH@.exe
